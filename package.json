{
  "name": "awsp",
  "version": "2.0.0",
  "description": "A script to switch between your AWS profiles",
  "main": "main.js",
  "files": [
    "src/"
  ],
  "bin": {
    "awsp": "src/main.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "debug": "cross-env PPTR_DEBUG=true node src/main.js",
    "postinstall": "cp -n .env.example .env || exit 0",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lintfix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexNaga/awsp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexNaga/awsp/issues"
  },
  "homepage": "https://github.com/AlexNaga/awsp#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fuzzy-search": "^3.2.1",
    "ini": "^3.0.1",
    "prompts": "^2.4.2",
    "puppeteer": "^17.1.3",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-block-resources": "^2.4.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,css,md}": "prettier --write"
  }
}
