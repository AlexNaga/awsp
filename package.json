{
  "name": "awsp",
  "version": "1.0.0",
  "description": "A script to switch between your AWS accounts",
  "main": "main.js",
  "files": [
    "src/"
  ],
  "bin": {
    "awsp": "src/main.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "debug": "cross-env PPTR_DEBUG=true node src/main.js",
    "postinstall": "cp -n .env.example .env || exit 0",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lintfix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexNaga/awsp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexNaga/awsp/issues"
  },
  "homepage": "https://github.com/AlexNaga/awsp#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fuzzy-search": "^3.2.1",
    "ini": "^2.0.0",
    "prompts": "^2.4.2",
    "puppeteer": "^10.4.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-block-resources": "^2.3.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,css,md}": "prettier --write"
  }
}
